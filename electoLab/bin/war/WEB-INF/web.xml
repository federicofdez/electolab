<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ElectoLab</servlet-name>
		<servlet-class>es.upm.dit.isst.electoLab20.ElectoLabServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ElectoLab</servlet-name>
		<url-pattern>/electolab</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>crearServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electoLab20.crearServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>crearServlet</servlet-name>
		<url-pattern>/crear</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>simularServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electoLab20.simularServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>simularServlet</servlet-name>
		<url-pattern>/simular</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>registrarServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electoLab20.registrarServlet</servlet-class>
		</servlet>
	<servlet-mapping>
		<servlet-name>registrarServlet</servlet-name>
		<url-pattern>/registrar</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>electolab</welcome-file>
	</welcome-file-list>
</web-app>
