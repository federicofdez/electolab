<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ElectoLab</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.ElectoLabServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ElectoLab</servlet-name>
		<url-pattern>/electolab</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CrearServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.CrearServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CrearServlet</servlet-name>
		<url-pattern>/crear</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SimularServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.SimularServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SimularServlet</servlet-name>
		<url-pattern>/simular</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RegistrarServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.RegistrarServlet</servlet-class>
		</servlet>
	<servlet-mapping>
		<servlet-name>RegistrarServlet</servlet-name>
		<url-pattern>/registrar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ForoServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.ForoServlet</servlet-class>
		</servlet>
	<servlet-mapping>
		<servlet-name>ForoServlet</servlet-name>
		<url-pattern>/foro</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>ForosimularServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.ForosimularServlet</servlet-class>
		</servlet>
	<servlet-mapping>
		<servlet-name>ForosimularServlet</servlet-name>
		<url-pattern>/forosimular</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>InitServlet</servlet-name>
		<servlet-class>es.upm.dit.isst.electolab.InitServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	
	<welcome-file-list>
		<welcome-file>electolab</welcome-file>
	</welcome-file-list>
</web-app>
